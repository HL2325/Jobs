/* pages/detailist/detailist.wxss */
.clear{
  clear: both;
}
.orderLists{
  width: 100%;
  height: 100%;
  margin-bottom: 40rpx;
}
.boardDetail{
  width: 80%;
  margin: 40rpx auto 0;
  font-size: 30rpx;
  border-radius: 20rpx;
  border:1px solid #dbdbdb;
  padding: 40rpx 20rpx 20rpx;
}
.detaiList{
  /* margin-bottom: 20rpx; */
  width: 100%;
  float: left;
}
.projectLabel {
  float: left;
  height: 60rpx;
}
.projectDetail{
  float: left;
  width: 75%;
}
.orderBtn{
  width: 30%;
  font-size: 30rpx;
  float: right;
}

.bodyMask{
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,.7);
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 11;
}
.maskCard{
  width: 80%;
  padding: 3% 5%;
  background: #fff;
  z-index: 12;
  position: absolute;
  top: 20%;
  left: 5%;
  border-radius: 10rpx;
  font-size: 30rpx;
}