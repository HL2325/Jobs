<!--pages/authentication/authentication.wxml-->
<view class='authen'>
  <view class='authenList'>
    <input class='authenInput' value='{{pro_title}}' bindinput='advPro_title' placeholder='项目名称' placeholder-style='font-size:28rpx;'></input>
  </view>
  <view class='authenList'>
    <input class='authenInput' value='{{realName}}' bindinput='advRealName' placeholder='真实姓名' placeholder-style='font-size:28rpx;'></input>
  </view>
  <view class='authenList'>
    <input class='authenInput' type='number' value='{{userTel}}' bindinput='advUserTel' placeholder='手机号码' placeholder-style='font-size:28rpx;'></input>
  </view>
  <view class='authenBtn' bindtap='getOriginator'>提交认证</view>
</view>
