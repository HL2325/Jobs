    <view class='fin_info'>
      <view class='finish_all'>
        <text class='fin_alls'>合计：</text>
        <text class='fin_num'>￥{{orderObj.good_price * orderObj.num}}</text>
      </view>
      <view class='finish_text'>含配送费</view>
    </view>
  </view> -->
</view>