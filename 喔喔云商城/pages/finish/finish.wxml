<!--pages/finish/finish.wxml-->
<view class='finish'>
  <view class='finish_top'>
    <view class='top_icon'>
      <image src='../../images/finish.png'></image>
    </view>
    <view class='top_text'>待领取</view>
    <view class='clear'></view>
  </view>
  <!-- 订单商品列表 -->
  <view class='commodity_list'>
    <view class='commodity_left'>
      <image src='{{orderObj.good_logo}}'></image>
    </view>
    <view class='commodity_right'>
      <view class='comityr_des'>{{orderObj.good_title}}</view>
      <view class='comityr_info'>
        <text class='comityr_price'>￥{{orderObj.good_price}}</text>
        <text class='comityr_num'>x{{orderObj.num}}</text>
      </view>
    </view>
    <view class='clear'></view>
  </view>
