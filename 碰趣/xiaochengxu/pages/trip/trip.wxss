/* pages/trip/trip.wxss */
@import '../../template/calendar/index.wxss';
.trip{
  overflow: hidden;
}
.trip .calendar-box{
  padding:59rpx 20rpx 20rpx;
  box-sizing: border-box; 
  background-color: #f5f5f5; 
  /* background-image: url('data:image/png;base64,'); */
  background-repeat: no-repeat;
  background-position: left top;
  background-size:100% 143rpx;
}


/* 路线 */
.trip .route{
  background:white;
  width:100%;
}
.trip .routescroll{ height:505rpx;  }
.trip .tree{
  padding: 28rpx 0 28rpx 30rpx;
  box-sizing: border-box;
  overflow: hidden;
  border-bottom:1rpx solid #bdbdbd;
  position: relative;
}
.trip .tree image{
  clear:both;
  float: left;
  height: 174rpx;
  width:174rpx;
  margin-right:20rpx;
}
.trip .tree text{
  flex: 1;
  width: 530rpx;
  height: 46rpx;
  font-size:32rpx;
  color:#333;
  text-align: left;
  line-height: 46rpx;
  overflow: hidden;
}

.trip .tree .text_con{
  display: flex;
 
  flex-direction: column;
  text-align: left

}

/* 底部 */
.buttom{
  height:98rpx;
  border-top:2rpx solid #dddddd;
  position:fixed;
  bottom:0;
  left:0;
}
.buttom .touchDay,.buttom .find,.buttom .my{
  width:250rpx;
  height:100%;
  float:left;
  text-align:center;
  font-size:20rpx;
  color:#666666;
  background:#fff;
}
.touchDay image{
  width:42rpx;
  height:42rpx;
  margin-top:20rpx;
  margin-bottom:10rpx;
}
.touchDay view{
  color:#fa5757;
}
.find image{
  width:49rpx;
  height:42rpx;
  margin-top:21rpx;
  margin-bottom:10rpx;
}
.my image{
  width:35rpx;
  height:40rpx;
  margin-top:22rpx;
  margin-bottom:10rpx;
}
