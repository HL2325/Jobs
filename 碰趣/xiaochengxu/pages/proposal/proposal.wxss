/* pages/proposal/proposal.wxss */
.proposal{
  width: 100%;
  height: auto;
}
.proposal_head{
  text-align: center;
  font-size: 50rpx;
  width: 100%;
  height: 150rpx;
  line-height: 150rpx;
  background: #f2f2f2;
}
.proposal_content{
  width: 90%;
  height: auto;
  margin: auto;
}
.proposal_content image{
  display: block;
  max-width: 100%;
  max-height: 100%;
  margin: auto;
  padding-top: 100rpx;
}
.text{
  display: block;
  width: 90%;
  height: auto;
  padding-top: 100rpx;
  text-align: center;
}