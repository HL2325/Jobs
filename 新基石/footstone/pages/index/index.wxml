<view class="home">
    <view wx:for="{{homeList}}" wx:key="{{item.id}}">
        <view class="homeList" data-id="{{item.id}}">
            <view class="listPic">
                <image src="{{item.avatar}}"></image>
            </view>
            <view class="listDes">
                <view class="originator" wx:if="{{item.realname}}">创始人：{{item.realname}}</view>
                <view class="originator" wx:else>创始人：{{item.nickname}}</view>
                <view>{{item.content}}</view>
                <view class="desPic">
                    <view class="picList" wx:for="{{item.picurl}}" wx:for-item="only" wx:key="index">
                        <image src="{{only}}"></image>
                    </view>
                    <view class="clear"></view>
                </view>
                <view class="clear"></view>
                <view style="font-size:28rpx;color:#666;margin-top:20rpx;">{{dateFr.getDate(item.create_time*1000,'-')}}</view>
            </view>
            <view class="clear"></view>
        </view>
        <view class="clear"></view>
    </view>
</view>
<wxs module="dateFr" src="../../utils/dateFr.wxs" />