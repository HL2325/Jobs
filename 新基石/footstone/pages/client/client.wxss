.client {
    width: 100%;
    height: 100%;
}

.clear {
    clear: both;
}

.clientList {
    border-bottom: 1px solid #d9d9d9;
    padding: 40rpx 30rpx;
}

.clientPic {
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
    background: #ccc;
    float: left;
    margin-right: 60rpx;
    overflow: hidden;
}

.clientPic image {
    width: 100%;
    height: 100%;
}

.clientText {
    font-size: 32rpx;
}