/* pages/angel/angel.wxss */
.clear{
  clear: both;
}
.angel{
  padding: 30rpx;
}
.angel_list{
  padding: 30rpx;
  border-bottom: 1px solid #f0f0f0;
  font-size: 30rpx;
  line-height: 50rpx;
  color: #333;
}
.angel_icon{
  width: 50rpx;
  height: 50rpx;
  float: left;
}
.angel_icon image{
  width: 100%;
  height: 100%;
}
.angel_text{
  float: left;
  margin-left: 40rpx;
}
.angel_num{
  float: right;
}
.rightIcon{
  width: 30rpx;
  height: 30rpx;
  margin-top: 10rpx;
}
.rightIcon image {
  width: 100%;
  height: 100%;
}