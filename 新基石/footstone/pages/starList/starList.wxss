/* pages/starList/starList.wxss */
page{
  width: 100%;
  min-height: 100vh;
}
.starList{
  min-height: 100vh;
  background-image: url('http://xinjishi.zhaodaka.vip/static/bg.jpg');
  background-size: 100% 100%;
  background-repeat: no-repeat;
}
.clear{
  clear: both;
}
.starPic{
  margin-top: 30rpx;
  width: 300rpx;
  height: 300rpx;
  float: left;
}
.starPic image{
  width: 100%;
  height: 100%;
}
.starPic:nth-of-type(2n){
  float: right;
}
.star_log{
  padding-top: 30rpx;
  padding-right: 30rpx;
  font-size: 30rpx;
  color: #dd5f1b;
  text-align: right;
}
.star_title{
  text-align: center;
  color: #fff;
  font-size: 32rpx;
}
.first_star{
  margin-top: 20rpx;
  text-align: center;
  font-size: 30rpx;
  color: #666;
}
.first_pic{
  width: 400rpx;
  height: 400rpx;
}
.bigList{
  padding: 0 50rpx;
  margin-top: 20rpx;
  padding-bottom: 20rpx;
}
.pic_list{
  float: left;
  margin-top: 20rpx;
  text-align: center;
  margin-left: 86rpx;
}
.li_pic{
  width: 160rpx;
  height: 160rpx;
}
.pic_list:nth-of-type(3n-2){
  margin-left: 0;
}
.pic_list:nth-of-type(3n){
  float: right;
  margin-left: 0;
}
.li_name{
  font-size: 28rpx;
  color: #fff;
}
.mask_pic{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,.8);
  display: flex;
  align-items: center;
}