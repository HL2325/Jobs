.clear {
  clear: both;
}

.home {
  width: 100%;
  height: 100%;
}

.homeList {
  padding: 20rpx;
  margin-top: 20rpx;
  border-bottom: 1px solid #d9d9d9;
}

.listPic {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  overflow: hidden;
  border: 1px solid #ccc;
  float: left;
}

.listPic image {
  width: 100%;
  height: 100%;
}

.listDes {
  width: 580rpx;
  float: right;
  font-size: 28rpx;
}

.originator {
  font-size: 32rpx;
  margin-bottom: 10rpx;
}

.desPic {
  margin-top: 20rpx;
}

.picList {
  width: 33.33%;
  float: left;
  height: 180rpx;
}

.picList image {
  width: 100%;
  height: 100%;
}

.pic_mask{
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  background: rgba(0,0,0,.8);
}

