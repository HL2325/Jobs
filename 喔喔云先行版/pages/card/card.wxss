/* pages/yearcard/yearcard.wxss */
page{
  background: #f6f6f6;
}
.clear{
  clear: both;
}
.card{
  border: 1px solid #f6f6f6;
}
.c_list{
  width: 690rpx;
  height: 386rpx;
  border-radius: 12rpx;
  overflow: hidden;
  margin: 30rpx auto 30rpx;
  box-shadow:0px 8px 16px 0px rgba(232,108,17,0.58);
  margin-bottom: 60rpx;
}
.cl_top{
  padding: 40rpx 40rpx 0 50rpx;
}
.cl_btit{
  font-size: 44rpx;
  font-weight: 600;
  color:rgba(255,255,255,1);
  line-height:62rpx;
  text-shadow:0px 3px 6px rgba(183,90,25,0.2);
  background:linear-gradient(180deg, rgba(255,255,255,1) 0%, rgba(249,239,211,0.75) 100%);
  -webkit-background-clip:text;
  -webkit-text-fill-color:transparent;
  float: left;
}
.allNum{
  float: right;
  font-size: 26rpx;
  font-weight: 400;
  color :rgba(255,255,255,1);
  line-height: 33rpx;
  text-shadow: 0rpx 3px 6px rgba(183,90,25,0.63);
  margin-top: 10rpx;
}
.create_date{
  margin-top: 200rpx;
  font-size: 26rpx;
  font-weight: 400;
  color: #fff;
  text-shadow:0px 3px 6px rgba(183,90,25,0.63);
  line-height: 33rpx;
  margin-left: 66rpx;
}